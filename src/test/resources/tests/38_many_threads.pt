// Test: Multiple threads executing concurrently
function work(id) do
    result = id * id
    return result
end

multi
    thread work(1) -> r1
    thread work(2) -> r2
    thread work(3) -> r3
    thread work(4) -> r4
    thread work(5) -> r5
end

PRINT(r1.value)
PRINT(r2.value)
PRINT(r3.value)
PRINT(r4.value)
PRINT(r5.value)
total = r1.value + r2.value + r3.value + r4.value + r5.value
PRINT(total)
