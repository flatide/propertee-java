// Test: Multiple threads executing concurrently
thread work(id) do
    result = id * id
    return result
end

multi
    work(1) -> r1
    work(2) -> r2
    work(3) -> r3
    work(4) -> r4
    work(5) -> r5
end

PRINT(r1.value)
PRINT(r2.value)
PRINT(r3.value)
PRINT(r4.value)
PRINT(r5.value)
total = r1.value + r2.value + r3.value + r4.value + r5.value
PRINT(total)
