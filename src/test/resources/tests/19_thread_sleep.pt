// Test: SLEEP in threads - sleeping thread doesn't block others
function sleeper() do
    PRINT("sleeper: before sleep")
    SLEEP(100)
    PRINT("sleeper: after sleep")
    return "slept"
end

function runner() do
    PRINT("runner: step 1")
    PRINT("runner: step 2")
    PRINT("runner: step 3")
    return "ran"
end

multi
    thread sleeper() -> r1
    thread runner() -> r2
end

PRINT("r1=" + r1.value)
PRINT("r2=" + r2.value)
