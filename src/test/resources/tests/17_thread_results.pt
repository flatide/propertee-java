// Test: Threads returning different types
function retNum() do
    return 42
end

function retStr() do
    return "hello"
end

function retArr() do
    return [1, 2, 3]
end

function retObj() do
    return { key: "value", num: 99 }
end

function retEmpty() do
    return
end

multi result do
    thread retNum() -> n
    thread retStr() -> s
    thread retArr() -> a
    thread retObj() -> o
    thread retEmpty() -> nl
end

PRINT(result.n.value)
PRINT(result.s.value)
PRINT(result.a.value)
PRINT(result.o.value.key)
PRINT(result.o.value.num)
PRINT(result.nl.value)
