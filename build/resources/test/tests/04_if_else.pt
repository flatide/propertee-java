// Test: If/else branching
x = 10

if x > 5 then
    PRINT("x > 5")
end

if x > 100 then
    PRINT("should not print")
end

if x == 10 then
    PRINT("x is 10")
else
    PRINT("x is not 10")
end

if x == 20 then
    PRINT("wrong")
else
    PRINT("correct branch")
end

// Nested if
y = 3
if y > 0 then
    if y < 10 then
        PRINT("y is between 0 and 10")
    end
end

// If with false boolean
if false then
    PRINT("should not print")
else
    PRINT("else branch")
end
