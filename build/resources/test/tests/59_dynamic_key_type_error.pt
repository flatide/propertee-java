function dummy(x) do
    return x * 10
end

key = 42
multi result do
    thread $(::key): dummy(1)
    thread $(true): dummy(2)
    thread 99: dummy(3)
    thread "named": dummy(4)
end

PRINT(result."42".value)
PRINT(result."true".value)
PRINT(result."99".value)
PRINT(result.named.value)
